{
  "expo": {
    "name": "sub",
    "slug": "sub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00c89c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juneceaser.sub",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app uses the microphone to enable voice input for transactions"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juneceaser.sub",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c062f74a-6aff-40b8-b8e8-28a7e2283b41"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
